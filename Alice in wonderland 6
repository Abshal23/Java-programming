import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        System.out.println("Bird said:"+a);
        int sum=a%10;
        int temp=a/10;
        int sum1=sum+temp;
        System.out.println("Alice must go in path-"+sum1);
        
    }
}
