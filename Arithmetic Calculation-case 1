import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        char c=sc.next().charAt(0);
        switch(c){
            case '+':
                float d=(float)a+b;
                System.out.println("Addition of two number is "+d);
                break;
            case '-':
                float e=(float)a-b;
                System.out.println("Subtraction of two number is "+e);
                break;
            case '*':
                float g=(float)a*b;
                System.out.println("Multiplication of two number is "+g);
                break;
            case '/':
                float h=(float)a/b;
                System.out.println("Division of two number is "+h);
                break;
            case '%':
                float i=(float)a%b;
                System.out.println("Modulo of two number is "+i);
                break;
            default:
                System.out.println("Invalid Input");
                break;
        }
        
    }
}
