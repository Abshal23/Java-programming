import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=c%60;
        int d1=c/60;
        int e=b+d1;
        int f=e%60;
        int g=e/60;
        int h=a+g;
        System.out.println("Total Number of hours is "+h);
        System.out.println("Total Number of minutes is "+f);
        System.out.println("Total Number of seconds is "+d);
        
        
    }
}
